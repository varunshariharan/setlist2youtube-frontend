{
  "name": "setlist2youtube-frontend",
  "version": "0.1.0",
  "description": "This repository contains the Chrome extension that converts a setlist from setlist.fm into a playable YouTube playlist.",
  "main": "index.js",
  "scripts": {
    "test": "vitest --run",
    "test:watch": "vitest",
    "build": "node scripts/build-manifest.mjs",
    "build:test": "GOOGLE_CLIENT_ID=test-client-id node scripts/build-manifest.mjs",
    "build:dev": "node scripts/build-manifest.mjs --env=dev",
    "setup:oauth": "echo 'Please see OAUTH-SETUP.md for detailed instructions'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/varunshariharan/setlist2youtube-frontend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/varunshariharan/setlist2youtube-frontend/issues"
  },
  "homepage": "https://github.com/varunshariharan/setlist2youtube-frontend#readme",
  "devDependencies": {
    "jsdom": "^26.1.0",
    "vitest": "^3.2.4"
  }
}